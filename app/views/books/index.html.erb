<div class="container">
  <div class="row">
    <div class="col-md-3">
      <h2>index of page</h2>
      <table>
        <thead>
          <tr>
            <th>title</th>
            <th>body</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @books.each do |book| %>
          <tr>
            <td><%= book.title %></td>
            <td><%= book.body %></td>
            <td><%= link_to "Show", book_path(book.id) %></td>
            <td><%= link_to "Edit",edit_book_path(book.id) %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>